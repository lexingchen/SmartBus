<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.test.mapper.DataMapper">

    <select id="datalist" resultType="com.example.test.bean.S_data">
        SELECT * FROM bus_data
    </select>
    <insert id="s_dataadd" parameterType="com.example.test.bean.S_data">
    insert into bus_data (devid,hum,tem,rain,north,east,datatime) values ( #{devid},#{hum},#{tem},#{rain},#{north},#{east},#{datatime})
    </insert>
    <select id="datashow" resultType="com.example.test.bean.S_data">
        select * from bus_data where id=(select max(id) from bus_data)
    </select>
    <select id="datalist3" resultType="com.example.test.bean.S_data">
        SELECT * FROM `bus_data` ORDER BY id DESC LIMIT 3
    </select>



</mapper>